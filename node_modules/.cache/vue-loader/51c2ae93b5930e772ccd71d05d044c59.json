class=\"fas fa-globe-africa\"></i></label>\n    </div>\n</template>\n\n<script>\n    import Task from './components/Task.vue';\n\n    export default {\n        name: 'app',\n        components: {\n            Task,\n        },\n        data() {\n            return {\n                inputTask: '',\n                sort: 'all',\n                tasks: [\n                    {\n                        id: 0,\n                        title: 'Faire les courses',\n                        isdone: false,\n                    },\n                ],\n            };\n        },\n        methods: {\n            addTask(e) {\n                if (e.code === 'Enter') {\n                    const newTask = {\n                        id: this.tasks.length,\n                        title: this.inputTask,\n                        isdone: false\n                    };\n                    this.tasks.push(newTask);\n                    this.inputTask = '';\n                }\n            },\n            onTaskChange(newTask) {\n                return this.tasks = [...this.tasks.filter(task => task.id !== newTask.id), newTask]\n            },\n            sortTask(e) {\n                this.sort = e.target.value;\n            },\n        },\n        computed: {\n            filterByDone() {\n                return this.tasks.filter(task => {\n                   switch (this.sort) {\n                       case 'all':\n                           return true;\n                       break;\n                       case 'done':\n                           if (!task.isdone) {\n                               return task;\n                           }\n                           break;\n                       case 'undone':\n                           if (task.isdone) {\n                               return task;\n                           }\n                       break;\n                   }\n                })\n            },\n        },\n    };\n</script>\n\n<style lang=\"scss\">\n    #app {\n        font-family: 'Avenir', Helvetica, Arial, sans-serif;\n        -webkit-font-smoothing: antialiased;\n        -moz-osx-font-smoothing: grayscale;\n        text-align: center;\n        color: #2c3e50;\n        margin-top: 60px;\n    }\n\n    #create-task {\n\n    }\n</style>\n"]}]}